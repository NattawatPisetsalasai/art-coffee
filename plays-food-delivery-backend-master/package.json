{
  "name": "test-plays-food-delivery",
  "version": "1.0.0",
  "description": "test backend for food delivery",
  "main": "./server/index.js",
  "scripts": {
    "server": "nodemon --ignore ./utils/gcs --exec babel-node ./server/index.js",
    "test": "nodemon --ignore ./utils/gcs --exec babel-node ./test/test.js",
    "test-route": "nodemon --exec babel-node ./test/testRoute.js",
    "clean": "run-scripts clean-build clean-package",
    "clean-build": "shx rm -rf dist && shx echo Clean build done",
    "prebuild": "npm run clean-build",
    "build": "cross-env-shell \"babel $APP_ENTRYPOINT --out-dir dist --copy-files\"",
    "clean-package": "shx rm -rf package && shx echo Clean package done",
    "prepackage": "run-scripts clean-package clean-build build",
    "package": "cross-env-shell \"pkg dist/$APP_DIST_ENTRYPOINT --targets node12-linux-x64 --output package/$APP_BIN_NAME && shx echo Build package done\"",
    "build:server": "cross-env APP_ENTRYPOINT=\"server\" npm run build",
    "package:server": "cross-env APP_ENTRYPOINT=\"server\" APP_DIST_ENTRYPOINT=\"index.js\" APP_BIN_NAME=\"test-expressjs\" npm run package",
    "gen-bucket-key-json": "node ./utils/gcs/genBucketKeyJson.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.7.0",
    "@hapi/boom": "^9.1.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "change-case": "^3.1.0",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "error-stack-parser": "^2.0.6",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-http-proxy": "^1.6.0",
    "express-validator": "^6.3.0",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^7.14.0",
    "pug": "^3.0.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2",
    "util": "^0.12.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "@wdio/appium-service": "^5.16.11",
    "@wdio/cli": "^5.16.13",
    "@wdio/local-runner": "^5.16.13",
    "@wdio/mocha-framework": "^5.16.13",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.14",
    "babel-loader": "^8.0.6",
    "chromedriver": "^75.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "pkg": "^4.4.8",
    "run-scripts": "^0.4.0",
    "shx": "^0.3.2",
    "wdio-chromedriver-service": "^5.0.2"
  }
}
